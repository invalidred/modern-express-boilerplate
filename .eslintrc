{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [ "prettier", "jest" ],
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "comma-dangle": "off",
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "parser": "flow"
    }]
  },
  "env": {
    "browser": false,
    "node": true,
    "jest/globals": true
  }
}
